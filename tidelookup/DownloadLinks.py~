for y in ['2015,2016,2017']:
    for p in ports.keys():
        cmd = 'wget http://www.linz.govt.nz/docs/hydro/tidal-info/tide-tables/maj-ports/csv/%s%20%s.csv -o %s/%s.csv'%(ports[p],y,p,y,)'
        print cmd
